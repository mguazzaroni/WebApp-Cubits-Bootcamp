@model List<Category>

<div class="row">
    <div class="col-lg-12">
        <h2>Lista de Categorias</h2>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Nombre</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th>@item.Id</th>
                        <th>@item.Name</th>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>